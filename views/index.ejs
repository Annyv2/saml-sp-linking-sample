<!doctype html>
<html>
  <head>
    <title>Auth0 SAML SP sample</title>
  </head>
  <body>
    <% if (locals.user) { %>
      <% if (user.user_id) { %>
        <p>Logged in as <%= user.user_id %> / <%= user.email %>.</p>
      <% } %>
    <p>Full profile:</p>
    <pre><%= JSON.stringify(user, null, 2) %></pre>
    <% } %>
    <% if (locals.account) { %>
    <p>Also logged in as <%= account.user_id %> / <%= account.email %>.</p>
    <% } else { %>
      <p><a href="/login">Click here to link another account</a></p>
    <% } %>
  </body>
</html>
